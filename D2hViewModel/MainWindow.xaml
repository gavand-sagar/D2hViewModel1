<Window x:Class="D2hViewModel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:D2hViewModel"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" Height="50">
            <Button Command="{Binding LoginCommand,Source={StaticResource MainWindowViewModel}}" Content="Login" />
            <Button Command="{Binding SignupCommand,Source={StaticResource MainWindowViewModel}}" Content="SignUp" />
        </StackPanel>
    </Grid>
</Window>
